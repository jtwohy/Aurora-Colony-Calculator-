        If String.IsNullOrWhiteSpace(CorQuant.Text) Then CoruQuant = 0 Else CoruQuant = Convert.ToDouble(CorQuant.Text)
        If String.IsNullOrWhiteSpace(CorAcc.Text) Then CoruAva = 0 Else CoruAva = Convert.ToDouble(CorAcc.Text)
        If String.IsNullOrWhiteSpace(GallQuan.Text) Then GallQuant = 0 Else GallQuant = Convert.ToDouble(GallQuan.Text)
        If String.IsNullOrWhiteSpace(GallAcc.Text) Then GallAva = 0 Else GallAva = Convert.ToDouble(GallAcc.Text)
        If String.IsNullOrWhiteSpace(ColonyCost.Text) Then Readout.Text = "Error enter a Colony Cost" Else ColCost = Convert.ToDouble(ColonyCost.Text)
        If String.IsNullOrWhiteSpace(Pop.Text) Then Readout.Text = "Error enter a Maximum Population" Else MaxPOP = Convert.ToDouble(Pop.Text)
        If String.IsNullOrWhiteSpace(BodyName.Text) Then Readout.Text = "Error enter the Name of the Celestial Body"

        Dim sDur As Double = DurAva * DurQaun
        Dim sNeu As Double = NeuAva * NeuQuan
        Dim sCorb As Double = CorbAva * CorbQuant
        Dim sTrit As Double = TritAva * TritQuant
        Dim sBor As Double = Borava * BorQuant
        Dim sMer As Double = MerAva * MerQuant
        Dim sVen As Double = VenAVA * VenQuant
        Dim sSor As Double = SorAva * SorQuan
        Dim sUr As Double = UrAva * UrQuant
        Dim sCoru As Double = CoruAva * CoruQuant
        Dim sGall As Double = GallAva * GallQuant
        Dim mineralscore As Double = sDur + sNeu + sCorb + sTrit + sBor + sMer + sVen + sSor + sUr + sCoru
        Dim Colonyscore As Double = mineralscore / ColCost
        If Colonyscore >= 250000 And MaxPOP >= 250 And ColCost < 10 Then
            Readout.Text = BodyName.Text + " is suitable for Colonization"
        ElseIf Colonyscore >= 250000 And (MaxPOP < 250 Or ColCost > 10) Then
            Readout.Text = BodyName.Text + " is Suitable for Automated mining"
        Else Readout.Text = BodyName.Text + " is unsuitable for colonization or automated mining"
        End If

    End Sub

